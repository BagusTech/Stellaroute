	-
		function uuid() {return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, (c) => { const r = Math.random()*16|0, v = c === 'x' ? r : (r&0x3|0x8);return v.toString(16);})}
		function randomizeArray(a, b) { var x = Math.random(); if(x>.5){return 1;}if(x<.5){return -1;}return 0;}
		function sortByOrder(a, b){const aOrder = parseInt(a.order, 10), bOrder = parseInt(b.order, 10); if(aOrder > bOrder){return 1} if(aOrder < bOrder){return -1} return 0; }
		function getIcon(icon) {
			const icons = {
				walk: 'fa fa-blind',
				car: 'fa fa-car',
				bicycle: 'fa fa-bicycle',
				bus: 'fa fa-bus',
				subway: 'fa fa-subway',
				train: 'fa fa-train',
				boat: 'fa fa-ship',
				plane: 'fa fa-plane',
				airport: 'fa fa-plane',
				attraction: 'fa fa-university',
				station: 'fa fa-train',
			};

			return icons[icon];
		}
		function userIsAuthorized() {
			if (!user) {
				return false;
			}

			return user.isAdmin
		}
		function wysiwygHasData(data) {
			const empytValues = [
				undefined,
				null,
				'<p><br></p>',
				'<div><br></div>',
				'<div><p><br></p></div>',
				'<br>',
			]

			return !(empytValues.indexOf(data) > -1)
		}
		function createValidAttributeFromTitle(title) {
			const findInvalidAttributeCharacters = /[^a-z0-9\s]|^[^a-z\s]+/gi;
			const findMultipleSpaces = /\s\s+/g;
			const findAllSpaces = /\s/g;

			return title.replace(findInvalidAttributeCharacters, '')
				.replace(findMultipleSpaces, ' ')
				.trim()
				.replace(findAllSpaces, '-');
		}
		function getImagePath(img, size) {
			if (!img) {
				return null;
			}

			const imgPath = img.split('.');
			const imgExtention = imgPath.pop();
			const s3Path = 'https://s3-us-west-2.amazonaws.com/stellaroute.com/images/';

			return img.indexOf('http') === 0 ? null : `${s3Path}${imgPath.join('')}-${size}.${imgExtention}`
		}

