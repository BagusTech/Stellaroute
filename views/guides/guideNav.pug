nav.interior-nav(data-function='interiorNav')
	button.interior-nav--toggle(data-interior-nav='toggle')
		i.fa.fa-chevron-right(aria-hidden='true')
	.interior-nav--nav(data-function='accordion', data-always-show-one='true')
		for card, i in cards ? cards : []
			if card.section && card.section.title
				a.interior-nav--node(href='#', role='tab', aria-expanded='false', data-position= i)
					h3= card.section.title
				if card.subCards && card.subCards.length
					ul.interior-nav--sub-nav(role='tabpanel', aria-hidden='true')
						for subCard, j in card.subCards ? card.subCards : []
							if subCard.title && (subCard.style === 'sub' || subCard.style === 'big')
								li(class=`guide-nav--${subCard.style}`): a.interior-nav--node(href='#', data-position= `${i}.${j}`)= subCard.title